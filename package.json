    "url": "https://github.com/aayushadhikari7/zammy-cli/issues"
  },
  "homepage": "https://github.com/aayushadhikari7/zammy-cli#readme",
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "figlet": "^1.7.0",
